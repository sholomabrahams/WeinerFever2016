{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/admin/**",
        "destination": "/admin/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
        "/style/less/**",
        "**/*.ini",
        "/bower_components/**/*@(map|json|md|gzip|lock|yml|less)"
    ]
  }
}
