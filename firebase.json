{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/admin/**",
        "destination": "/admin/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
        "/style/less/**",
        "**/*.ini",
        "/bower_components/**/*@(json|md|gzip|lock|yml|less)",
        "/assets/img/team_pics/**",
        "/assets/img/team_pics - 4 3/**",
        "/assets/img/team_pics_old/**"
    ]
  }
}
